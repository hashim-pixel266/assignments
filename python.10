# 3Ô∏è‚É£ Marks list to dict with Pass/Fail and type
marks = ["55", "45", "85", "35"]
results = {}
for mark in marks:
    score = int(mark)
    results[score] = "Pass" if score >= 40 else "Fail"
for k, v in results.items():
    print(k, "->", v, "|", type(k), type(v))

# 4Ô∏è‚É£ Tuple to dict with type casting and +5 to score
tup = ("Farah", "20", "88.5")
data = {
    "name": tup[0],
    "age": int(tup[1]),
    "score": float(tup[2]) + 5
}
