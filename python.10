# 5Ô∏è‚É£ Count names > 4 letters, name ‚ûù length dict, label short/medium/long
names = ["Ana", "Sana", "Salman"]
lengths = {}
for name in names:
    l = len(name)
    label = "short" if l <= 4 else "medium" if l <= 6 else "long"
    lengths[name] = (l, label)
print("Names with length > 4:", sum(1 for n in names if len(n) > 4))
for k, v in lengths.items():
    print(k, "->", v)

# 6Ô∏è‚É£ Input product names & prices, highlight if > 100
products = {}
for i in range(3):
    prod = input("Enter product name: ")
    price = float(input(f"Enter price of {prod}: "))
    products[prod] = price
for item, price in products.items():
    tag = "üî∫ Expensive" if price > 100 else "‚úÖ Reasonable"
    print(item, "->", price, tag)

# 7Ô∏è‚É£ Mixed list to float, categorize above/below 250
mixed = ["100", 200, 300.5, "400"]
for item in mixed:
    val = float(item)
    label = "Above 250" if val > 250 else "Below 250"
    print(val, "->", label)

# 8Ô∏è‚É£ Tuple conversion and dict + student check
